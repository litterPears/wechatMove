.cinema{
  padding:0 30rpx;
  height:auto;
  overflow:hidden;
}
.positonDiv{
  width:100%;
  height:80rpx;
}
.tables{
  width:100%;
  height:80rpx;
  line-height:80rpx;
  border-bottom:#aaa;
  background:#fff;
  position:fixed;
  top:10rpx;
  left:0;
  display:flex;
  justify-content:space-around;
  align-items:center;
  background:#999;
}
.span{
  height:80rpx;
  padding:0 20rpx;
  color:#ffbe00;
  font-size:30rpx;
}
.cinemaMain{
  display:flex;
  justify-content:flex-start;
  height:auto;
  margin-bottom:50rpx;
}
.cinemaList{
  width:50%;
  height:200rpx;
  display:flex;
  justify-content:space-around;
  align-items:center;
}